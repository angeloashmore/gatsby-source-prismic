{
  "name": "gatsby-source-prismic",
  "version": "3.0.0-beta.19",
  "description": "Gatsby source plugin for building websites using prismic.io as a data source",
  "main": "index.cjs.js",
  "module": "index.esm.js",
  "sideEffects": false,
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup -c",
    "build:watch": "npm run build -- --watch",
    "format": "prettier --write 'src/**/*.js' 'rollup.config.js' 'docs/**/*.md'",
    "prebuild": "rimraf dist",
    "prepare": "npm run build",
    "release": "standard-version",
    "test": "jest",
    "test:watch": "npm run test -- --watch"
  },
  "homepage": "https://github.com/angeloashmore/gatsby-source-prismic",
  "repository": {
    "type": "git",
    "url": "https://github.com/angeloashmore/gatsby-source-prismic.git"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source-plugin",
    "prismic"
  ],
  "author": "Angelo Ashmore <angelo.ashmore@walltowall.com>",
  "license": "MIT",
  "dependencies": {
    "camelcase": "^5.3.1",
    "compose-tiny": "^1.1.3",
    "es-cookie": "^1.3.0",
    "fs-extra": "^8.1.0",
    "gatsby-source-filesystem": "^2.1.39",
    "lodash.clonedeep": "^4.5.0",
    "lodash.mergewith": "^4.6.2",
    "md5": "^2.2.1",
    "pascalcase": "^1.0.0",
    "prismic-dom": "^2.1.0",
    "prismic-javascript": "^2.1.1",
    "query-string": "^6.9.0",
    "ramda": "^0.26.1",
    "ramda-adjunct": "^2.23.0",
    "traverse": "^0.6.6",
    "uuid": "^3.3.3",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@testing-library/react-hooks": "^3.2.1",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-plugin-ramda": "^2.0.0",
    "cross-env": "^6.0.3",
    "eslint": "^6.7.1",
    "eslint-plugin-jest": "^23.0.5",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-test-renderer": "^16.12.0",
    "rimraf": "^3.0.0",
    "rollup": "^1.27.5",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-terser": "^5.1.2",
    "standard-version": "^7.0.1"
  },
  "peerDependencies": {
    "gatsby": ">=2.3",
    "react": ">=16.8"
  },
  "browserslist": ">1%, not ie 11, not op_mini all",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
